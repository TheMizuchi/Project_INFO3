TestTreeCond (Init) {
	* (Init):
		| (Key(z) & Key(d)) / !(Key(q)) ? : (Init)
}

TestActionMultiple (Init) {
	* (Init):
		| True ? 40%Move(F) / Move / Move : (Init)
}

Test1 (Init) {
	* (Init):
		| Key(z) ? Move(N) / Move(N) : (Moving)
		| Key(q) ? Move(W) : (Moving)
		| Key(s) ? Move(S) : (Moving)
		| Key(d) ? Move(E) : (Moving)
		| True ? Move : (Init)
	* (Moving):
		| Key(z) ? Move(N) : (Moving)
		| Key(q) ? Move(W) : (Moving)
		| Key(s) ? Move(S) : (Moving)
		| Key(d) ? Move(E) : (Moving)
		| True ? : (Init)
}

Mine (Init) {
	* (Init):
		| Cell(F, V) ? Move(F) : (Init)
		| True ? : (Waiting)
	* (Waiting):
		| Key(e) ? Pop / Hit / Power / Jump : ()
		| True ? : (Waiting)
}